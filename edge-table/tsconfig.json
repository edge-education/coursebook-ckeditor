{
  "compilerOptions": {
    "types": [],
    "lib": [
      "ES2019",
      "ES2020.String",
      "DOM",
      "DOM.Iterable"
    ],
    "outDir": "./build",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "strict": true,
    "module": "es6",
    "target": "es2019",
    "sourceMap": true,
    "declaration": true,
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "./build/**/*"
  ]
}



